{
  "name": "billingmate-admin-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start-dev": "vite --mode development",
    "build-dev": "tsc &&  vite build --mode development",
    "preview-dev": "vite preview  --mode development",
    "start-qa": "vite --mode qa",
